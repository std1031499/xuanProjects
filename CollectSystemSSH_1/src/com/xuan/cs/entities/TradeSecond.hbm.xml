<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.xuan.cs.entities.TradeSecond" table="TB_TRADESECOND">
		<id name="tradeSecondId" type="java.lang.String">
			<column name="TRADESECOND_ID"></column>
			<generator class="uuid"/>
		</id>
		
		<property name="tradeSecondName"  type="java.lang.String">
			<column name="TRADESECOND_NAME"></column>
		</property>
	</class>
</hibernate-mapping>
