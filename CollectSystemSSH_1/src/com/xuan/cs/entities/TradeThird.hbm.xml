<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.xuan.cs.entities.TradeThird" table="TB_TRADETHIRD">
		<id name="tradeThirdId" type="java.lang.String">
			<column name="TRADETHIRD_ID"></column>
			<generator class="uuid"/>
		</id>
		
		<property name="tradeThirdName"  type="java.lang.String">
			<column name="TRADETHIRD_NAME"></column>
		</property>
	</class>
</hibernate-mapping>
